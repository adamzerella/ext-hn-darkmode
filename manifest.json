{
    "name": "HackerNews Dark Theme",
    "short_name": "hndt",
    "version": "0.1.0",
    "description": "Switch to a Dark Theme for the popular website Hacker News.",
    "permissions": [ "activeTab" ],
    "background": {
      "scripts": ["extension.js"],
