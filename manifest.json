{
    "name": "Hacker News Dark Theme",
    "short_name": "hndt",
    "version": "0.1.0",
    "description": "A non-invaise dark theme for Hacker News.",
    "permissions": [ "activeTab" ],
    "background": {
      "scripts": ["extension.js"],
      "persistent": false
    },
    "browser_action": {
        "default_title": "Hacker News Dark Theme"
    },
    "web_accessible_resources" : [ "extension.js" ],
    "manifest_version": 2
  }